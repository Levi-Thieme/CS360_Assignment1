package models;

import java.util.ArrayList;

/**
 * The Database Class stores the Watershed Objects
 * @author Levi Thieme
 *
 */
public class Database {
	private ArrayList<Watershed>[] entries;
	
	private int rows;
	private int columns;
	private static final String[] columnNames = {"ID",
            "Name",
            "Latitude",
            "Longitude",
            "Updated",
            "Description"};
	
	
	public ArrayList<Watershed>[] getEntries() {
		return entries;
	}
	public int getRows() {
		return rows;
	}
	public int getColumns() {
		return columns;
	}
	public String[] getColumnNames() {
		return columnNames;
	}
	
}
